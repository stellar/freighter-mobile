{
  "name": "freighter-mobile",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "android": "yarn android-dev",
    "android-dev": "ruby scripts/launch-android --env=dev",
    "android-prod": "ruby scripts/launch-android --env=prod",
    "android-dev-release": "ruby scripts/launch-android --env=dev --release",
    "android-prod-release": "ruby scripts/launch-android --env=prod --release",
    "android-clean": "yarn android-dev-clean",
    "android-dev-clean": "ruby scripts/launch-android --env=dev --clean",
    "android-prod-clean": "ruby scripts/launch-android --env=prod --clean",
    "android-dev-release-clean": "ruby scripts/launch-android --env=dev --release --clean",
    "android-prod-release-clean": "ruby scripts/launch-android --env=prod --release --clean",
    "android:bundle:dev-release": "cd android && ./gradlew bundleDevRelease",
    "android:bundle:prod-release": "cd android && ./gradlew bundleProdRelease",
    "ios": "yarn ios-dev",
    "ios-dev": "yarn ios-dev-sim",
    "ios-prod": "yarn ios-prod-sim",
    "ios-device": "yarn ios-dev-device",
    "ios-dev-sim": "react-native run-ios --scheme=freighter-mobile-dev --simulator=\"$(./scripts/get-ios-simulator)\"",
    "ios-prod-sim": "react-native run-ios --scheme=freighter-mobile --simulator=\"$(./scripts/get-ios-simulator)\"",
    "ios-dev-device": "react-native run-ios --scheme=freighter-mobile-dev --device",
    "ios-prod-device": "react-native run-ios --scheme=freighter-mobile --device",
    "lint": "eslint ./src ./__tests__ --fix",
    "lint:ts": "tsc --noEmit",
    "lint:ts:watch": "tsc --watch --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:check": "eslint ./src ./__tests__",
    "lint:translations": "eslint ./src --rule 'translations/missing-translations: error'",
    "clean:translations": "node scripts/clean-translations.js",
    "check": "yarn lint:ts && yarn lint:check && yarn format:check",
    "fix": "yarn lint && yarn format",
    "postinstall": "husky && rn-nodeify --install fs,url,events,https,http,util,stream,crypto,vm,buffer --hack --yarn && patch-package && node scripts/setup-scrypt-16kb.js && yarn pod-install",
    "start": "react-native start",
    "start-c": "yarn start --reset-cache",
    "pod-install": "sh ./scripts/pod-install",
    "test": "jest",
    "test:watch": "jest --watch",
    "node-clean": "sh ./scripts/node-clean",
    "gradle-clean": "sh ./scripts/gradle-clean",
    "node-c-install": "yarn node-clean && yarn install",
    "gradle-c-install": "yarn gradle-clean && yarn install",
    "c-install": "yarn gradle-clean && yarn node-clean && yarn install",
    "reset-env": "sh ./scripts/reset-env",
    "r-install": "yarn reset-env && yarn install",
    "generate-splashscreen": "yarn react-native-bootsplash generate splashscreen/freighter-logo.png --platforms=android,ios --background=161616 --logo-width=206 --assets-output=assets/bootsplash --flavor=main"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
